{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "display": {
    "separator": "  "
  },
  "logo": {
    "type": "kitty-direct",
    "source": "/home/okt/Files/Wallpapers/scratch/LinuxLogoMono.png",
    "width": 40,
    "padding": {
      "top": 2,
      "left": 1,
      "right": 1
    }
  },
  "modules": [
    "break",
    "break",
    {
      "type": "os",
      "key": "[OS.]",
      "format": "{pretty-name}",
      "keyColor": "31"
    },
    {
      "type": "kernel",
      "key": "[KER]",
      "format": "{2}",
      "keyColor": "32"
    },
    {
      "type": "packages",
      "format": "{all}",
      "key": "[PKG]",
      "keyColor": "33"
    },
    {
      "type": "shell",
      "format": "{3}",
      "key": "[SH.]",
      "keyColor": "34"
    },
    {
      "type": "terminal",
      "format": "{5}",
      "key": "[TER]",
      "keyColor": "35"
    },
    {
      "type": "wm",
      "key": "[WM.]",
      "keyColor": "36"
    },
    "break",
    "colors",
    "break"
  ]
}
